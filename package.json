{
    "name": "vue-blog-template",
    "version": "2.0.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "d3": "^5.7.0",
        "element-ui": "^2.4.7",
        "html2canvas": "^1.0.0-alpha.12",
        "js-cookie": "^2.2.0",
        "jszip": "^3.2.1",
        "mockjs": "^1.0.1-beta3",
        "nifti-reader-js": "^0.5.4",
        "three": "^0.125.0",
        "vue": "^2.5.17",
        "vue-i18n": "^8.4.0",
        "vue-image-crop-upload": "^2.5.0",
        "vue-position-sticky": "^0.2.0",
        "vue-progressbar": "^0.7.5",
        "vue-router": "^3.0.1",
        "vuex": "^3.0.1"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.0.0",
        "@vue/cli-plugin-eslint": "^3.0.0",
        "@vue/cli-service": "^3.0.0",
        "@vue/eslint-config-standard": "^3.0.4",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.2",
        "vue-template-compiler": "^2.5.17"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "@vue/standard"
        ],
        "rules": {
            "no-undef": 0,
            "indent": 0,
            "camelcase": 0,
            "space-before-function-paren": 0
        },
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
